{
  "name": "@mygov/mygov",
  "description": "MyGov.org.nz",
  "version": "4.0.0",
  "private": true,
  "homepage": "https://mygov.org.nz",
  "author": "Keri Henare <keri@henare.co.nz>",
  "license": "MIT",
  "main": "build/worker/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mygov-nz/mygov.org.nz.git"
  },
  "bugs": {
    "url": "https://github.com/mygov-nz/mygov.org.nz/issues"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "sideEffects": false,
  "scripts": {
    "dev-build": "rm -rf build && npm run build:static && npm run build:service-worker && npm run build:webworker",
    "build": "rm -rf build && NODE_ENV=production npm run build:static && NODE_ENV=production npm run build:service-worker && NODE_ENV=production npm run build:webworker",
    "build:service-worker": "babel ./src/public/js/service-worker.ts --config-file ./babel.service-worker.js --out-file ./build/public/service-worker.js",
    "build:static": "NODE_ENV=production webpack --config webpack.static.js --mode production --progress",
    "build:webworker": "NODE_ENV=production webpack --config webpack.webworker.js --mode production --progress",
    "generate-types": "tsm src/components",
    "start": "wrangler dev",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:lint": "eslint --ext .ts,.tsx src",
    "test:type-check": "tsc"
  },
  "dependencies": {
    "@cloudflare/kv-asset-handler": "^0.1.3",
    "gallagher": "^1.1.0",
    "history": "^5.0.1",
    "mem": "^9.0.1",
    "preact": "^10.5.14",
    "preact-render-to-string": "^5.1.19",
    "saintelague": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@cloudflare/workers-types": "^2.0.0",
    "@types/history": "^4.7.7",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-console-source": "^2.0.5",
    "babel-plugin-minify-replace": "^0.5.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-preset-minify": "^0.5.1",
    "compression-webpack-plugin": "^9.0.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.3.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "cssnano": "^5.0.8",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-security": "^1.4.0",
    "faster.js": "^1.1.1",
    "fibers": "^5.0.0",
    "file-loader": "^6.0.0",
    "jest": "^27.2.0",
    "mini-css-extract-plugin": "^2.3.0",
    "null-loader": "^4.0.1",
    "postcss": "^8.3.6",
    "prettier": "^2.1.0",
    "sass": "^1.41.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "terser-webpack-plugin": "^5.2.4",
    "ts-jest": "^27.0.5",
    "typed-scss-modules": "^4.1.1",
    "types-cloudflare-worker": "^1.2.0",
    "typescript": "^4.0.2",
    "webpack": "^5.53.0",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-merge": "^5.1.2",
    "webpack-pwa-manifest": "^4.2.0"
  }
}
